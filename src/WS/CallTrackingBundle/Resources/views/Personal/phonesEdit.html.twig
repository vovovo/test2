{% extends 'personal_layout.html.twig' %}
{% block body %}
    <div>{{ message }}</div>

<div class="row">
        <div class="col-md-6">
            <div class="box box-info">
    {{ form_start(form ) }}
    {{ form_errors(form) }}

                <div class="box-body">
                    <div class="form-group">
                        <label>{{ form_label(form.utm) }}</label>
                        {{ form_errors(form.utm) }}
                        {{ form_widget(form.utm,{ 'attr': {'class': 'form-control'} }) }}
                    </div>

                    <div class="form-group">
                        <label>{{ form_label(form.phone) }}</label>
                        {{ form_errors(form.phone) }}
                        {{ form_widget(form.phone,{ 'attr': {'class': 'form-control'} }) }}
                    </div>
                </div>
                <div class="box-footer">
                    {{ form_widget(form.save,{ 'attr': {'class': 'btn btn-primary', value:'save'} }) }}
                </div>
    {{ form_end(form) }}
            </div>
        </div>
</div>


{% endblock %}

